{
    "name": "slm/queue-amq",
    "description": "Zend Framework 2 module that integrates with Active MQ queuing system",
    "license": "BSD-3-Clause",
    "type": "library",
    "keywords": [
        "zf2",
        "queue",
        "job",
        "amq",
        "active-mq",
        "activemq",
        "stomp"
    ],
    "homepage": "https://github.com/juriansluiman/SlmQueueAmq",
    "authors": [
        {
            "name": "Jurian Sluiman",
            "email": "jurian@juriansluiman.nl",
            "homepage": "http://juriansluiman.nl"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "zendframework/zend-mvc": "^2.2",
        "zendframework/zend-servicemanager": "^2.7.6 || ^3.0",
        "zendframework/zend-stdlib": "^2.2",
        "slm/queue": "^1.0",
        "fusesource/stomp-php": "~2.1"
    },
    "require-dev": {
        "zendframework/zend-modulemanager": "^2.5",
        "zendframework/zend-view": "^2.5",
        "zendframework/zend-serializer": "^2.5",
        "zendframework/zend-log": "^2.5",
        "zendframework/zend-i18n": "^2.5",
        "squizlabs/php_codesniffer": "1.5.*",
        "phpunit/phpunit": "~4.1"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "SlmQueueAmq\\": "src"
        },
        "classmap": [
            "./Module.php"
        ]
    }
}
